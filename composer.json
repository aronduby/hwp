{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "thetispro/laravel5-setting": "1.1.x-dev",
        "barryvdh/laravel-debugbar": "^3.2",
        "eluceo/ical": "^0.10.1",
        "fusonic/opengraph": "@dev",
        "fusonic/linq": "@dev",
        "predis/predis": "~1.0",
        "illuminated/console-logger": "5.6.*",
        "guzzlehttp/guzzle": "^6.2",
        "fabpot/goutte": "^3.1",
        "symfony/dom-crawler": "^3.1",
        "symfony/css-selector": "^3.1",
        "laravel-notification-channels/twitter": "^1.0",
        "doctrine/dbal": "^2.5",
        "p3k/emoji-detector": "^0.2.1",
        "twilio/sdk": "^5.36",
        "torzer/awesome-landlord": "^3.0",
        "laravel/tinker": "^1.0",
        "monolog/monolog": "1.23",
        "laravel/passport": "^7.5",
        "benbjurstrom/passport-custom-jwt-claims": "dev-master",
        "ybr-nx/laravel-mariadb": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-ide-helper": "2.4.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {},
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aronduby/passport-custom-jwt-claims"
        }
    ]
}
